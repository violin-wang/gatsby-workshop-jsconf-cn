{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.5.0",
    "@wework/ray-core": "^1.12.3",
    "@wework/ray-core-react": "^1.12.4",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.21",
    "gatsby-background-image": "^0.8.12",
    "gatsby-image": "^2.2.21",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-mdx": "^1.0.44",
    "gatsby-plugin-offline": "^3.0.9",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-remote-images": "^1.0.7",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-plugin-styled-components": "^3.1.6",
    "gatsby-remark-images": "^3.1.23",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-transformer-json": "^2.2.9",
    "gatsby-transformer-sharp": "^2.2.17",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "husky": "^3.0.5",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check \"**/*.{js,jsx,json,md}\""
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
